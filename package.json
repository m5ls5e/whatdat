{
  "name": "whatdat.app",
  "version": "0.10.56",
  "description": "What Dat?!?",
  "main": "src/index.html",
  "private": true,
  "scripts": {
    "build": "yarn clean && yarn webpack",
    "build:dev": "yarn clean && yarn webpack:dev",
    "clean": "rm -rf ./dist > /dev/null 2>&1",
    "serve": "yarn webpack:serve",
    "start": "yarn serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ver:bump": "yarn version --no-git-tag-version",
    "ver:patch": "yarn ver:bump --patch && yarn ver:tag && yarn ver:publish",
    "ver:minor": "yarn ver:bump --minor && yarn ver:tag && yarn ver:publish",
    "ver:major": "yarn ver:bump --major && yarn ver:tag && yarn ver:publish",
    "ver:publish": "git push --all && git push --tags",
    "ver:tag": "APP_VERSION=$(node -e 'console.log(process.env.npm_package_version)') && git add package.json && git commit -m \":bookmark: Bump version v$APP_VERSION\" && git tag $APP_VERSION -m \"Bump version v$APP_VERSION\"",
    "webpack": "cross-env NODE_ENV=production webpack --color",
    "webpack:dev": "cross-env NODE_ENV=development webpack --color",
    "webpack:serve": "cross-env NODE_ENV=development webpack serve"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.coms"
  },
  "repository": "github:mikesprague/whatdat",
  "license": "MIT",
  "engines": {
    "node": ">= 12.x",
    "npm": ">= 6.x",
    "yarn": ">=1.x"
  },
  "dependencies": {
    "@bugsnag/js": "^7.16.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/pro-duotone-svg-icons": "^6.1.1",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@tensorflow-models/coco-ssd": "^2.2.2",
    "@tensorflow-models/mobilenet": "^2.1.0",
    "@tensorflow/tfjs": "^3.16.0",
    "@tensorflow/tfjs-backend-cpu": "^3.16.0",
    "@tensorflow/tfjs-backend-webgl": "^3.16.0",
    "@tensorflow/tfjs-converter": "^3.16.0",
    "@tensorflow/tfjs-core": "^3.16.0",
    "bootstrap": "^5.1.3",
    "bootswatch": "^5.1.3",
    "cross-env": "^7.0.3",
    "fabric": "^5.2.1",
    "html2canvas": "^1.4.1",
    "logrocket": "^3.0.0",
    "register-service-worker": "^1.7.2",
    "sweetalert2": "^11.4.10",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/eslint-plugin": "^7.17.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-object-assign": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/runtime": "^7.17.9",
    "@popperjs/core": "^2.11.5",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "autoprefixer": "^10.4.5",
    "babel-loader": "^8.2.5",
    "compression-webpack-plugin": "^9.2.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cssnano": "^5.1.7",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.13",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.6.2",
    "rollup": "^2.71.1",
    "sass-lint": "^1.13.1",
    "sass-loader": "^12.6.0",
    "seedrandom": "^2.4.3",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-bugsnag-plugin": "^0.1.4",
    "webpack-bugsnag-plugins": "^1.8.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^3.11.2",
    "workbox-webpack-plugin": "^6.5.3"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-object-assign"
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": {
    "development": [
      "last 1 version"
    ],
    "production": [
      "> 2%",
      "last 2 versions",
      "not ie <= 10"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "airbnb-base"
    ],
    "plugins": [
      "compat",
      "html",
      "import",
      "markdown",
      "prettier",
      "security"
    ],
    "parserOptions": {
      "parser": "babel-parser",
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true
      }
    },
    "rules": {
      "curly": "error",
      "object-curly-newline": "error",
      "semi": "error",
      "prefer-arrow-callback": "warn",
      "arrow-spacing": "error",
      "eqeqeq": "error",
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ],
      "max-len": [
        "warn",
        {
          "code": 120,
          "tabWidth": 4,
          "ignoreUrls": true
        }
      ],
      "import/no-cycle": [
        "warn",
        {
          "maxDepth": 1
        }
      ],
      "import/prefer-default-export": "off"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "semi": true,
    "tabWidth": 2,
    "plugins": [
      "all"
    ],
    "printWidth": 120
  },
  "snyk": true
}
