{
  "name": "whatdat.app",
  "version": "0.3.6",
  "description": "What Dat?!?",
  "main": "src/index.html",
  "private": true,
  "scripts": {
    "build": "yarn clean && yarn webpack",
    "build:dev": "yarn dev:clean && yarn webpack:dev",
    "clean": "rm -rf ./dist > /dev/null 2>&1",
    "serve": "yarn webpack:serve",
    "start": "yarn serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ver:bump": "yarn version --no-git-tag-version",
    "ver:patch": "yarn ver:bump --patch && yarn ver:tag && yarn ver:publish",
    "ver:minor": "yarn ver:bump --minor && yarn ver:tag && yarn ver:publish",
    "ver:major": "yarn ver:bump --major && yarn ver:tag && yarn ver:publish",
    "ver:publish": "git push --all && git push --tags",
    "ver:tag": "APP_VERSION=$(node -p \"require('./package.json').version\") && git add package.json && git commit -s -m \":bookmark: Bump version v$APP_VERSION\" && git tag $APP_VERSION -s -m \"Bump version v$APP_VERSION\"",
    "webpack": "cross-env NODE_ENV=production webpack --config ./webpack.config.js -p --colors --display minimal",
    "webpack:dev": "cross-env NODE_ENV=development webpack --config ./webpack.config.js -d --colors --display minimal",
    "webpack:serve": "cross-env NODE_ENV=development webpack-dev-server --port 3000",
    "snyk-protect": "snyk protect",
    "prepublish": "yarn snyk-protect"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.coms"
  },
  "repository": "github:mikesprague/whatdat",
  "license": "MIT",
  "engines": {
    "node": ">= 11.x",
    "npm": ">= 6.x",
    "yarn": ">=1.x"
  },
  "dependencies": {
    "@bugsnag/js": "^6.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/pro-light-svg-icons": "^5.9.0",
    "@fortawesome/pro-regular-svg-icons": "^5.9.0",
    "@fortawesome/pro-solid-svg-icons": "^5.9.0",
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "@tensorflow-models/coco-ssd": "^1.0.2",
    "@tensorflow-models/mobilenet": "^1.0.1",
    "@tensorflow/tfjs": "^1.1.2",
    "autoprefixer": "^9.6.0",
    "bootstrap": "^4.3.1",
    "bootswatch": "^4.3.1",
    "cssnano": "^4.1.10",
    "logrocket": "^1.0.0",
    "register-service-worker": "^1.6.2",
    "rollup": "^1.15.1",
    "snyk": "^1.177.0",
    "sweetalert2": "^8.12.1",
    "tippy.js": "^4.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "babel-loader": "^8.0.6",
    "compression-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "sass-lint": "^1.13.1",
    "sass-loader": "^7.1.0",
    "seedrandom": "^2.4.4",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.33.0",
    "webpack-bugsnag-plugins": "^1.4.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpackbar": "^3.2.0",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-object-assign"
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "airbnb-base"
    ],
    "plugins": [
      "compat",
      "html",
      "import",
      "markdown",
      "prettier"
    ],
    "parserOptions": {
      "parser": "babel-parser",
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true
      }
    },
    "rules": {
      "curly": "error",
      "object-curly-newline": "error",
      "semi": "error",
      "prefer-arrow-callback": "warn",
      "arrow-spacing": "error",
      "eqeqeq": "error",
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ],
      "max-len": [
        "warn",
        {
          "code": 120,
          "tabWidth": 4,
          "ignoreUrls": true
        }
      ],
      "import/no-cycle": [
        "warn",
        {
          "maxDepth": 1
        }
      ],
      "import/prefer-default-export": "off"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "semi": true,
    "tabWidth": 2,
    "plugins": [
      "all"
    ],
    "printWidth": 120
  }
}
