{
  "name": "whatdat.app",
  "version": "0.13.3",
  "description": "What Dat?!?",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf ./build",
    "dev": "vite serve",
    "prebuild": "npm run clean",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ver:bump": "npm version --git-tag-version false",
    "ver:patch": "npm run ver:bump -- patch && npm run ver:tag && npm run ver:publish",
    "ver:minor": "npm run ver:bump -- minor && npm run ver:tag && npm run ver:publish",
    "ver:major": "npm run ver:bump -- major && npm run ver:tag && npm run ver:publish",
    "ver:publish": "git push --all && git push --tags",
    "ver:tag": "APP_VERSION=$(node -e 'console.log(process.env.npm_package_version)') && git add package.json package-lock.json && git commit -m \"chore(release): bump version v$APP_VERSION\" && git tag $APP_VERSION -m \"chore(release): v$APP_VERSION\""
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.coms"
  },
  "repository": "github:mikesprague/whatdat",
  "license": "MIT",
  "engines": {
    "node": ">= 16.x",
    "npm": ">= 8.x"
  },
  "dependencies": {
    "@bugsnag/js": "^7.20.2",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/pro-duotone-svg-icons": "^6.4.0",
    "@tensorflow-models/coco-ssd": "^2.2.2",
    "@tensorflow-models/mobilenet": "^2.1.0",
    "@tensorflow/tfjs": "^4.9.0",
    "@tensorflow/tfjs-backend-cpu": "^4.9.0",
    "@tensorflow/tfjs-backend-webgl": "^4.9.0",
    "@tensorflow/tfjs-converter": "^4.9.0",
    "@tensorflow/tfjs-core": "^4.9.0",
    "bootstrap": "^5.3.1",
    "bootswatch": "^5.3.1",
    "fabric": "^5.3.0",
    "html2canvas": "^1.4.1",
    "logrocket": "^4.0.4",
    "sweetalert2": "^11.7.20",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.9",
    "@babel/eslint-plugin": "^7.22.5",
    "@babel/preset-env": "^7.22.9",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-compat": "^4.1.4",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-security": "^1.7.1",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "prettier-eslint": "^15.0.1",
    "prettier-stylelint": "^0.4.2",
    "sass": "^1.64.1",
    "seedrandom": "^3.0.5",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-prettier": "^4.0.2",
    "vite": "^4.4.7",
    "vite-plugin-pwa": "^0.16.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": {
    "development": [
      "last 1 version"
    ],
    "production": [
      "> 1%",
      "last 2 versions",
      "not ie <= 10"
    ]
  },
  "eslintConfig": {
    "settings": {
      "jest": {
        "version": 28
      }
    },
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "prettier",
      "stylelint"
    ],
    "plugins": [
      "babel",
      "compat",
      "html",
      "import",
      "prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "ecmaVersion": 11,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true
      }
    },
    "rules": {
      "node/no-unpublished-import": "warn",
      "node/no-missing-import": "warn"
    },
    "ignorePatterns": [
      "dist/",
      ".history/",
      "node_modules/"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "tabWidth": 2
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
