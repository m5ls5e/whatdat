{
  "name": "whatdat.app",
  "version": "0.5.0",
  "description": "What Dat?!?",
  "main": "src/index.html",
  "private": true,
  "scripts": {
    "build": "yarn clean && yarn webpack",
    "build:dev": "yarn dev:clean && yarn webpack:dev",
    "clean": "rm -rf ./dist > /dev/null 2>&1",
    "serve": "yarn webpack:serve",
    "start": "yarn serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ver:bump": "yarn version --no-git-tag-version",
    "ver:patch": "yarn ver:bump --patch && yarn ver:tag && yarn ver:publish",
    "ver:minor": "yarn ver:bump --minor && yarn ver:tag && yarn ver:publish",
    "ver:major": "yarn ver:bump --major && yarn ver:tag && yarn ver:publish",
    "ver:publish": "git push --all && git push --tags",
    "ver:tag": "APP_VERSION=$(node -p \"require('./package.json').version\") && git add package.json && git commit -s -m \":bookmark: Bump version v$APP_VERSION\" && git tag $APP_VERSION -s -m \"Bump version v$APP_VERSION\"",
    "webpack": "cross-env NODE_ENV=production webpack --config ./webpack.config.js -p --colors --display minimal",
    "webpack:dev": "cross-env NODE_ENV=development webpack --config ./webpack.config.js -d --colors --display minimal",
    "webpack:serve": "cross-env NODE_ENV=development webpack-dev-server --port 4000",
    "snyk-protect": "snyk protect",
    "prepublish": "yarn snyk-protect"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.coms"
  },
  "repository": "github:mikesprague/whatdat",
  "license": "MIT",
  "engines": {
    "node": ">= 12.x",
    "npm": ">= 6.x",
    "yarn": ">=1.x"
  },
  "dependencies": {
    "@bugsnag/js": "^6.5.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/pro-duotone-svg-icons": "^5.13.0",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@tensorflow-models/coco-ssd": "^2.0.1",
    "@tensorflow-models/mobilenet": "^2.0.4",
    "@tensorflow/tfjs": "^1.7.1",
    "@tensorflow/tfjs-converter": "^1.7.1",
    "@tensorflow/tfjs-core": "^1.7.1",
    "autoprefixer": "^9.7.5",
    "bootstrap": "^4.4.1",
    "bootswatch": "^4.4.1",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "fabric": "^3.6.3",
    "html2canvas": "^1.0.0-rc.5",
    "logrocket": "^1.0.7",
    "register-service-worker": "^1.7.1",
    "rollup": "^2.2.0",
    "snyk": "^1.305.0",
    "sweetalert2": "^9.10.7",
    "tippy.js": "^6.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^3.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-security": "^1.4.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.2",
    "sass-lint": "^1.13.1",
    "sass-loader": "^8.0.2",
    "seedrandom": "^2.4.4",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.1",
    "webpack-bugsnag-plugins": "^1.4.3",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpackbar": "^4.0.0",
    "workbox-webpack-plugin": "^5.1.2"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-object-assign"
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": {
    "development": [
      "last 1 version"
    ],
    "production": [
      "> 1%",
      "last 2 versions",
      "not ie <= 10"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "airbnb-base"
    ],
    "plugins": [
      "compat",
      "html",
      "import",
      "markdown",
      "prettier",
      "security"
    ],
    "parserOptions": {
      "parser": "babel-parser",
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true
      }
    },
    "rules": {
      "curly": "error",
      "object-curly-newline": "error",
      "semi": "error",
      "prefer-arrow-callback": "warn",
      "arrow-spacing": "error",
      "eqeqeq": "error",
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ],
      "max-len": [
        "warn",
        {
          "code": 120,
          "tabWidth": 4,
          "ignoreUrls": true
        }
      ],
      "import/no-cycle": [
        "warn",
        {
          "maxDepth": 1
        }
      ],
      "import/prefer-default-export": "off"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "semi": true,
    "tabWidth": 2,
    "plugins": [
      "all"
    ],
    "printWidth": 120
  },
  "snyk": true
}
