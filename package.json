{
  "name": "whatdat.app",
  "version": "0.0.1",
  "description": "What Dat?!?",
  "main": "src/index.html",
  "private": true,
  "scripts": {
    "build": "yarn clean && yarn webpack",
    "build:dev": "yarn dev:clean && yarn webpack:dev",
    "clean": "rm -rf ./dist > /dev/null 2>&1",
    "serve": "yarn webpack:serve",
    "start": "yarn serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack --config ./webpack.config.js -p --colors --display minimal",
    "webpack:dev": "webpack --config ./webpack.config.js -d --colors --display minimal",
    "webpack:serve": "webpack-serve ./webpack.config.js --port 3000"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.coms"
  },
  "repository": "github:mikesprague/whatdat",
  "license": "MIT",
  "engines": {
    "node": ">= 11.x",
    "npm": ">= 6.x",
    "yarn": ">=1.x"
  },
  "dependencies": {
    "@bugsnag/js": "^6.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/pro-light-svg-icons": "^5.8.1",
    "@fortawesome/pro-regular-svg-icons": "^5.8.1",
    "@fortawesome/pro-solid-svg-icons": "^5.8.1",
    "@fullhuman/postcss-purgecss": "^1.1.0",
    "@tensorflow/tfjs": "^1.0.2",
    "@tensorflow-models/coco-ssd": "^1.0.0",
    "@tensorflow-models/mobilenet": "^1.0.0",
    "autoprefixer": "^9.5.0",
    "bootstrap": "^4.3.1",
    "bootswatch": "^4.3.1",
    "cssnano": "^4.1.10",
    "logrocket": "^0.6.19"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/runtime": "^7.4.2",
    "babel-loader": "^8.0.5",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-compat": "^3.1.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.29.6",
    "webpack-bugsnag-plugins": "^1.4.0",
    "webpack-cli": "^3.3.0",
    "webpack-serve": "^2.0.3",
    "webpackbar": "^3.1.5"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-object-assign"
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": {
    "development": ["last 2 versions"],
    "production": ["last 3 versions", "IE >= 11", "Safari >= 9"]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "airbnb-base"
    ],
    "plugins": [
      "compat",
      "html",
      "import",
      "markdown",
      "prettier"
    ],
    "parserOptions": {
      "parser": "babel-parser",
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true
      }
    },
    "rules": {
      "curly": "error",
      "object-curly-newline": "error",
      "semi": "error",
      "prefer-arrow-callback": "warn",
      "arrow-spacing": "error",
      "eqeqeq": "error",
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ],
      "max-len": [
        "warn",
        {
          "code": 120,
          "tabWidth": 4,
          "ignoreUrls": true
        }
      ],
      "import/no-cycle": [
        "warn",
        {
          "maxDepth": 1
        }
      ],
      "import/prefer-default-export": "off"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "semi": true,
    "tabWidth": 2,
    "plugins": [
      "all"
    ],
    "printWidth": 120
  }
}
